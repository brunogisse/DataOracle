        'UME_RECEBIDO_MES, v.estoqueid as estoque_id_vendas, v.status_fre' +
        'te, v.status_corretagem, v.status_pgto_cliente, '
      ''
      
        '  c.nome as corretor, m.nome as motorista, p.NOME_FANTASIA as po' +
        'sto, pr.descricao as produto, re.nome as representante, u.NOME_F' +
        'ANTASIA as usina,'
      ''
      '  eu.estoqueid'
      ''
      ''
      'from'
      ''
      
        ' venda_para_postos v, corretor c, motorista m, posto p, produto ' +
        'pr, representante re, usina u, estoque_usina eu'
      ''
      ''
      'where'
