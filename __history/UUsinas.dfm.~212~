    UpdateOptions.AssignedValues = [uvGeneratorName]
    UpdateOptions.GeneratorName = 'GEN_ESTOQUE_USINA_ID'
    UpdateOptions.UpdateTableName = 'ESTOQUE_USINA'
    UpdateOptions.KeyFields = 'ESTOQUEID'
    UpdateOptions.AutoIncFields = 'ESTOQUEID'
    SQL.Strings = (
      'select'
      
        '   eu.estoqueid, eu.usinaid, eu.produtoid, eu.representanteid, e' +
        'u.estoque,'
      '   pr.produtoid, pr.descricao,'
      '   r.representanteid, r.nome'
      ''
      'from'
      '  estoque_usina eu, produto pr, representante r'
      ''
      'where'
      '   (eu.produtoid = pr.produtoid) and'
