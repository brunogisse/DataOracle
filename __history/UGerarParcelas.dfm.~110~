  object qryParcelas: TFDQuery
    Connection = frmMenu.FDconexao
    Transaction = tcParcelas
    UpdateOptions.AssignedValues = [uvGeneratorName]
    UpdateOptions.GeneratorName = 'GEN_PARCELA_VENDA_PARA_POSTOS_I'
    UpdateOptions.UpdateTableName = 'PARCELA_VENDA_PARA_POSTOS'
    UpdateOptions.KeyFields = 'PARCELAID'
    UpdateOptions.AutoIncFields = 'PARCELAID'
    SQL.Strings = (
      ' SELECT * from parcela_venda_para_postos PARC, forma_pgto FP'
      ''
      ' where'
      ''
      ' (PARC.forma_pgto_id = FP.formaid) and'
      ' (vendaid = :vendaid) and'
      ' (status = '#39'ABERTO'#39')')
    Left = 243
    Top = 299
    ParamData = <
      item
        Name = 'VENDAID'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
